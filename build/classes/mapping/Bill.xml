<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="mapping">
	
	    <insert id="addBill" parameterType="Bill" >   
	        insert into bill(bill_id,medical_record_id,bill_type ,amount_of_money,bill_resource,delete_mark) 
	        values(#{bill_id},#{medical_record_id},#{bill_type},#{amount_of_money},#{bill_resource},#{delete_mark})
	    </insert>
	  

	
	</mapper>
	
