<?xml version="1.0" encoding="UTF-8"?>



<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="mapping">
	
	   
	    <select id="getDrugs" resultType="Drug">       
	        select * from drug  ;
	    </select>
	    

	        
	    <select id="get_drug_by_id" parameterType="String" resultType="Drug">
	        select * from  drug where drug_id= #{drug_id}   ; 
	    </select>
	        

	    
	</mapper>